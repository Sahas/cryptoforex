<?xml version="1.0"?>
<ExchangeApiDef exchange="Binance" xmlns="urn:Crypto.Forex"
	xmlns:xsi="urn:Crypto.Forex">

	<type>api</type>
	<baseUri>https://api.binance.com</baseUri>
	<priceUri>api/v3/ticker/bookTicker</priceUri>
	<price>
		<attr_name>bidPrice</attr_name>
	</price>
	<baseCoin>
		<attr_name>symbol</attr_name>
		<operation>
			<name>determine</name>
			<args>0</args>
		</operation>
	</baseCoin>

	<peggedCoin>
		<attr_name>symbol</attr_name>
		<operation>
			<name>determine</name>
			<args>1</args>
		</operation>
	</peggedCoin>

</ExchangeApiDef>